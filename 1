#include <iostream>
using namespace std;
int main()
{
    int a, b, c, d, k;
    cin>>a>>b;
    setlocale(LC_ALL, " ");
    cout<<"Число: ";
    cin>>c;
    cout<<"Кол-во повторов: ";
    cin>>d;
    k=0;
    while(a!=(b+1))
    {
        cout<<a;
        a+=1;
        while(k!=(d-1))
        {
            cout<<a;
            k+=1;
        }
    }
    return 0;
}
